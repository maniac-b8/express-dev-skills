<%- include('../partials/header') %>
    <ul>
      <a href="/skills/new">Add a skill</a>
        <% skills.forEach(function(s) { %>
        <li>
          <form action="/skills/<%= s.id %>?_method=DELETE"
            class="delete-form" method="POST">
            <button type="submit">X</button>
          </form>
            <a href="/skills/ <%= s.id %>">
                <%= s.skill %>
            </a>
        </li>
        <% }) %>
    </ul>
    <h4><a href="/">Home</a></h4>
<%- include('../partials/footer') %>